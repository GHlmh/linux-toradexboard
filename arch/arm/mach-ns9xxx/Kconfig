	bool "SPI port D"
	default n
	depends on SPI_NS9360 && !CCX9C_FB

config CCX9C_FB
	bool "Framebuffer"
	default y
	depends on FB_NS9360

config CCX9C_TOUCH
	bool "Touchscreen"
	default y
	depends on TOUCHSCREEN_ADS7846 && CCX9C_FB && CCX9C_SPI_PORTB

endif

