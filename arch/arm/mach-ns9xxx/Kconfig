config CCX9C_SERIAL_PORTB_RXTX
	bool "RX/TX only"

config CCX9C_SERIAL_PORTB_CTSRTSRXTX
	bool "CTS/RTS/RX/TX"

config CCX9C_SERIAL_PORTB_FULL
	bool "Full port"

endchoice

